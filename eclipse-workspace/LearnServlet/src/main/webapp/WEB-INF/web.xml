<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>LearnServlet</display-name>
 
  <servlet>
  	<servlet-name>calc</servlet-name>
  	<servlet-class>com.calculator.Calculator</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>calc</servlet-name>
  	<url-pattern>/calculator</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-name>square</servlet-name>
  	<servlet-class>com.calculator.Calculator</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>square</servlet-name>
  	<url-pattern>/square</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>reqDispatcher</servlet-name>
  	<servlet-class>com.calculator.ReqDispatcherExample</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>reqDispatcher</servlet-name>
  	<url-pattern>/reqDis</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  	<servlet-name>printDispatcher</servlet-name>
  	<servlet-class>com.calculator.PrintDispatcherClass</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>printDispatcher</servlet-name>
  	<url-pattern>/printDispatcher</url-pattern>
  </servlet-mapping>
  
  
</web-app>